{
	"name": "Cuppa",
	"type": "dark",
	"colors": {
		// Activity Bar
        "activityBar.foreground": "#da9b4f",
        "activityBar.inactiveForeground": "#f4d29c",
        "activityBar.background": "#1a1a1a",
        "activityBar.activeBackground": "#111111",
        "activityBar.activeBorder": "#da9b4f",
        "activityBarBadge.foreground": "#111111",
        "activityBarBadge.background": "#da9b4f",

        // Notifications
        "notifications.background": "#222222",
        "notifications.foreground": "#f4d29c",
        "notificationsErrorIcon.foreground": "#da9b4f",
        "notificationsInfoIcon.foreground": "#da9b4f",
        "notificationsWarningIcon.foreground": "#da9b4f",
        "notificationToast.border": "#da9b4f",

        // Sidebar
        "sideBarSectionHeader.background": "#1a1a1a",
        "sideBarSectionHeader.foreground": "#da9b4f",
        "sideBar.background": "#222222",
        "sideBarTitle.foreground": "#da9b4f",
        "sideBar.foreground": "#f4d29c",
        "sideBar.border": "#da9b4f",

        // Dropdowns
        "dropdown.background": "#222222",
        "dropdown.foreground": "#f4d29c",
        "dropdown.border": "#111111",

        // Terminal
        "terminal.background": "#141414",
        "terminal.foreground": "#f4d29c",
        "terminalCursor.foreground": "#da9b4f",
        "terminal.selectionBackground": "#da9b4f",
        "terminal.ansiBrightYellow": "#fff4d7",

        // Title Bar
        "titleBar.activeBackground": "#1a1a1a",
        "titleBar.activeForeground": "#f4d29c",
        "titleBar.inactiveBackground": "#222222",
        "titleBar.inactiveForeground": "#da9b4f",

        // Tabs
        "tab.activeBackground": "#1a1a1a",
        "tab.activeForeground": "#da9b4f",
        "tab.border": "#1a1a1a",
        "tab.activeBorder": "#2a2a2a",
        "tab.activeBorderTop": "#da9b4f",
        "tab.inactiveBackground": "#222222",
        "tab.inactiveForeground": "#f4d29c",

        // Panel (on top of terminal)
        "panel.background": "#222222",
        "panel.border": "#da9b4f",
        "panelTitle.activeForeground": "#da9b4f",
        "panelTitle.activeBorder": "#da9b4f",
        "panelTitle.inactiveForeground": "#f4d29c",

        // Status Bar
        "statusBar.background": "#111111",
        "statusBar.foreground": "#f4d29c",

        // Editor
        "editor.background": "#222222",
        "editor.foreground": "#f4d29c",
        "editorCursor.foreground": "#da9b4f",
        "editorLineNumber.foreground": "#f4d29c",
        "editorLineNumber.activeForeground": "#dd8c2a",
        "editorLink.activeForeground": "#dd8c2a",
        "editorGroupHeader.tabsBackground": "#1a1a1a",

        // Breadcrumbs
        "breadcrumb.background": "#1b1b1b",
        "breadcrumb.focusForeground": "#f4d29c",
        "breadcrumb.foreground": "#da9b4f",

		// Misc
        "foreground": "#f4d29c",
		"list.activeSelectionBackground": "#1c1c1c",
		"list.activeSelectionForeground": "#da9b4f",
		"input.background": "#111111",
		"input.foreground": "#f4d29c",
		"focusBorder": "#da9b4f",
		"input.border": "#f4d29c",
		"list.dropBackground": "#1a1a1a",
		"list.hoverBackground": "#1c1c1c"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#73BAFA"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#A8D7FF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF8282"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#f8e1bc"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#f8e1bc"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#da9b4f"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#f8e1bc"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f4d29c"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f4d29c"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#f4d29c"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#da9b4f"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#f7c472"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#f4d29c"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#f4d29c"
			}
